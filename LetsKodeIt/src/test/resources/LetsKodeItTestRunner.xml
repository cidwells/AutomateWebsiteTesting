<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="LetsKodeIt"    parallel="false">

    <test name="LetsKodeItTest using firefox">
    <parameter name="useCloudEnv" value="false "/>
    <parameter name="cloudEnvName" value="browserstack"/>
    <parameter name="os" value="windows"/>
    <parameter name="os_version" value="10"/>
    <parameter name="browserName" value="firefox"/>
    <parameter name="browserVersion" value="86.0"/>
    <parameter name="url" value="https://letskodeit.teachable.com/"/>
        <classes>
            <class name = "testLetsKodeItPractice.LetsKodeItPracticeTest"/>
        </classes>
    </test>
    <test name="LetsKodeItTest using chrome">
        <parameter name="useCloudEnv" value="false "/>
        <parameter name="cloudEnvName" value="browserstack"/>
        <parameter name="os" value="windows"/>
        <parameter name="os_version" value="10"/>
        <parameter name="browserName" value="chrome"/>
        <parameter name="browserVersion" value="86.0"/>
        <parameter name="url" value="https://letskodeit.teachable.com/"/>
        <classes>
            <class name = "testLetsKodeItPractice.LetsKodeItPracticeTest"/>
        </classes>
    </test>
</suite>