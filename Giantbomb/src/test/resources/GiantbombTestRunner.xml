<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">

<suite name="Giantbomb " parallel="false">

    <parameter name="useCloudEnv" value="false"/>
    <parameter name="cloudEnvName" value="browserstack"/>
    <parameter name="os" value="windows"/>
    <parameter name="os_version" value="10"/>
    <parameter name="browserName" value="chrome"/>
    <parameter name="browserVersion" value="86.0"/>
    <parameter name="url" value="https://www.giantbomb.com/"/>


    <test name = "GiantbombTest">
        <classes>
            <class name="testHome.GiantbombHomeTest"/>
            <class name="testSearch.GiantbombTestSearch"/>
        </classes>
    </test>

</suite>
